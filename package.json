{
  "name": "websharp-compiler",
  "version": "0.1.0",
  "description": "Web# Programming Language Compiler - C# syntax for browser-native development",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
